package me.twammer.domain

class Twam {
	
	String message
	String timePosted
	String fromWho
	User user
	

    static constraints = {
		message(nullable:false)
		timePosted(nullable:false)
		fromWho(nullable:false)
		user(nullable:true)
    }
	
	static mapping = {
		user lazy:true // don't load user with each twam.
	}
	
}
